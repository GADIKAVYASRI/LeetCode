class Solution {
    public int mostWordsFound(String[] sentences) {
        int ans=0;
        for(int i=0;i<sentences.length;i++){
            int a=1;
            char ch[]=sentences[i].toCharArray();
            for(int j=0;j<ch.length;j++){
                if(ch[j]==' '){
                a++;
            }
            }
            ans=Math.max(ans,a);
        }
        return ans;
    }
}
