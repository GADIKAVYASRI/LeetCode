class Solution {
    public int maximumDifference(int[] nums) {
        int diff=0;
        int n=nums.length;
        int i=0;
        int d=nums[0];
        for(int j=1;j<n;j++){ 
            if(nums[i]<nums[j]){
                d=Math.min(nums[i],d);
                diff=Math.max((nums[j]-d),diff);
            }
            i++;
        }
        if(diff==0){
            diff=-1;
        }
        return diff;
    }
}
