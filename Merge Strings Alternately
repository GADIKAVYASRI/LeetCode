class Solution {
    public String mergeAlternately(String word1, String word2) {
        boolean bool=true;
        int m=word1.length();
        int n=word2.length();
        String s="";
        for(int i=0;i<m+n;i++){
            if(i<m){
                s+=word1.charAt(i);
            }
            if(i<n){
                s+=word2.charAt(i);
            }
        }
        return s;
    }
}
